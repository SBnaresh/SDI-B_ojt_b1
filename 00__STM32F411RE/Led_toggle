#include <stdint.h>
#include<stdio.h>
#include"stm32f4xx.h"

void Delay(int T)
{
int i;
  while(T--)
  {
for(i=0;i<5000;i++);
  }

}
int main()
{

RCC->AHB1ENR|=7;

GPIOA->MODER|= 0X1000;

GPIOB->MODER|= 0X1000;

GPIOC->MODER|= 0X40000;

while(1)
   {
	GPIOA->ODR|= 0x100;
	Delay(100);
	GPIOB->ODR|=0x100;
	Delay(100);
	GPIOC->ODR|=0x200;
	Delay(200);

	GPIOA->ODR &=~0x100;
	 Delay(100);
	GPIOB->ODR &=~0x100;
	Delay(100);
	GPIOC->ODR &=~0x200;
	Delay(200);
	}
}
